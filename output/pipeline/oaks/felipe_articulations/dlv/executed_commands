euler2 check ./examples/oaks/taxonomies.txt ./examples/oaks/felipe_articulations/articulations.txt
euler2 align ./examples/oaks/taxonomies.txt ./examples/oaks/felipe_articulations/articulations.txt --consistency -o ./output/pipeline/oaks/felipe_articulations/dlv -r dlv
euler2 show iv ./examples/oaks/taxonomies.txt ./examples/oaks/felipe_articulations/articulations.txt -o ./output/pipeline/oaks/felipe_articulations/dlv
euler2 align ./examples/oaks/taxonomies.txt ./examples/oaks/felipe_articulations/articulations.txt -o ./output/pipeline/oaks/felipe_articulations/dlv -r dlv
euler2 show -o ./output/pipeline/oaks/felipe_articulations/dlv pw
euler2 show -o ./output/pipeline/oaks/felipe_articulations/dlv sv
