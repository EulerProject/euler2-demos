euler2 check ./examples/oaks/taxonomies.txt ./examples/oaks/erica_articulations/articulations.txt
euler2 align ./examples/oaks/taxonomies.txt ./examples/oaks/erica_articulations/articulations.txt --consistency -o ./output/pipeline/oaks/erica_articulations
euler2 show iv ./examples/oaks/taxonomies.txt ./examples/oaks/erica_articulations/articulations.txt -o ./output/pipeline/oaks/erica_articulations
euler2 align ./examples/oaks/taxonomies.txt ./examples/oaks/erica_articulations/articulations.txt -o ./output/pipeline/oaks/erica_articulations
euler2 show -o ./output/pipeline/oaks/erica_articulations pw
euler2 show -o ./output/pipeline/oaks/erica_articulations sv
