euler2 check ./examples/oaks/taxonomies.txt ./examples/oaks/erica_articulations/articulations.txt
euler2 align ./examples/oaks/taxonomies.txt ./examples/oaks/erica_articulations/articulations.txt --consistency -o ./output/pipeline/oaks/erica_articulations/gringo -r gringo
euler2 show iv ./examples/oaks/taxonomies.txt ./examples/oaks/erica_articulations/articulations.txt -o ./output/pipeline/oaks/erica_articulations/gringo --svg
euler2 align ./examples/oaks/taxonomies.txt ./examples/oaks/erica_articulations/articulations.txt -o ./output/pipeline/oaks/erica_articulations/gringo -r gringo
euler2 show -o ./output/pipeline/oaks/erica_articulations/gringo pw --svg
euler2 show -o ./output/pipeline/oaks/erica_articulations/gringo sv --svg
