euler2 check ./examples/oaks/taxonomies.txt ./examples/oaks/harry_articulations/articulations.txt
euler2 align ./examples/oaks/taxonomies.txt ./examples/oaks/harry_articulations/articulations.txt --consistency -o ./output/pipeline/oaks/harry_articulations/dlv -r dlv
euler2 show iv ./examples/oaks/taxonomies.txt ./examples/oaks/harry_articulations/articulations.txt -o ./output/pipeline/oaks/harry_articulations/dlv --svg
euler2 align ./examples/oaks/taxonomies.txt ./examples/oaks/harry_articulations/articulations.txt -o ./output/pipeline/oaks/harry_articulations/dlv -r dlv
euler2 show -o ./output/pipeline/oaks/harry_articulations/dlv pw --svg
euler2 show -o ./output/pipeline/oaks/harry_articulations/dlv sv --svg
