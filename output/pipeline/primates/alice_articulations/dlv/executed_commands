euler2 check ./examples/primates/taxonomies.txt ./examples/primates/alice_articulations/articulations.txt
euler2 align ./examples/primates/taxonomies.txt ./examples/primates/alice_articulations/articulations.txt --consistency -o ./output/pipeline/primates/alice_articulations/dlv -r dlv
euler2 show iv ./examples/primates/taxonomies.txt ./examples/primates/alice_articulations/articulations.txt -o ./output/pipeline/primates/alice_articulations/dlv --svg
euler2 align ./examples/primates/taxonomies.txt ./examples/primates/alice_articulations/articulations.txt -o ./output/pipeline/primates/alice_articulations/dlv -r dlv
euler2 show -o ./output/pipeline/primates/alice_articulations/dlv pw --svg
euler2 show -o ./output/pipeline/primates/alice_articulations/dlv sv --svg
