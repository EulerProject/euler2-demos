input_check:
 euler2 check {.}/datasets/{dataset}/input.txt
consistency_check:
 euler2 align {.}/datasets/{dataset}/input.txt --consistency -o {.}/output/{demo}/{dataset}
input_viusalization:
 euler2 show {.}/datasets/{dataset}/input.txt iv -o {.}/output/{demo}/{dataset}
align:
 euler2 align {.}/datasets/{dataset}/input.txt -o {.}/output/{demo}/{dataset}
show_possible_worlds:
 euler2 show -o {.}/output/{demo}/{dataset} pw
show_aggregate_view:
 euler2 show -o {.}/output/{demo}/{dataset} sv
 
