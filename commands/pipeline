input_check:
 euler2 check {.}/examples/{example}/taxonomies.txt {.}/examples/{example}/{articulations}/articulations.txt
consistency_check:
 euler2 align {.}/examples/{example}/taxonomies.txt {.}/examples/{example}/{articulations}/articulations.txt --consistency -o {.}/output/{commands}/{example}/{articulations}/{reasoner} -r {reasoner}
input_viusalization:
 euler2 show iv {.}/examples/{example}/taxonomies.txt {.}/examples/{example}/{articulations}/articulations.txt -o {.}/output/{commands}/{example}/{articulations}/{reasoner}
align:
 euler2 align {.}/examples/{example}/taxonomies.txt {.}/examples/{example}/{articulations}/articulations.txt -o {.}/output/{commands}/{example}/{articulations}/{reasoner} -r {reasoner}
show_possible_worlds:
 euler2 show -o {.}/output/{commands}/{example}/{articulations}/{reasoner} pw
show_summary_view:
 euler2 show -o {.}/output/{commands}/{example}/{articulations}/{reasoner} sv
 
