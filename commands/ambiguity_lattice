align_art_rem:
 command: euler2 align {.}/examples/{example}/taxonomies.txt {.}/examples/{example}/{articulations}/articulations.txt -o {.}/output/{commands}/{example}/{articulations}/{reasoner}/{encoding} --artRem -r {reasoner} -e {encoding}
 doc: 
show_ambiguity_lattice:
 command: euler2 show -o {.}/output/{commands}/{example}/{articulations}/{reasoner}/{encoding} ambLat --svg
